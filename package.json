{
  "name": "vision-matcher",
  "version": "1.1.2",
  "description": "Vision Matcher is a project that provides a face matching utility",
  "main": "dist/index.js",
  "scripts": {
    "prettier": "prettier '**/*.+(js|json|ts|tsx|md|yml|yaml)'",
    "format": "pnpm prettier --write",
    "check-types": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "check-format": "pnpm prettier --list-different",
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpileOnly src/index.ts",
    "lint": "pnpm eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "mocha --exit --recursive 'test/**/*.test.ts'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "async": "^3.2.6",
    "axios": "^1.7.9",
    "express": "^4.21.2",
    "body-parser": "^1.20.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/async": "^3.2.24",
    "@types/body-parser": "^1.19.5",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "body-parser": "^1.20.2",
    "chai": "^5.1.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.9",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-unused-imports": "^4.1.4",
    "mocha": "^10.8.2",
    "prettier": "^3.5.3",
    "supertest": "^7.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.2"
  },
  "packageManager": "pnpm@9.15.3"
}
